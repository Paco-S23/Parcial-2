#include <stdio.h>

int main (){
	
	int v1,v2,v3;
	
	printf("Valor 1:");
	scanf("%d",&v1);
	
	printf("Valor 2:");
	scanf("%d",&v2);
	
	printf("Valor 3:");
	scanf("%d",&v3);
	
	printf("\nValores originales:\n1:%d \n2:%d \n3:%d\n\n",v1,v2,v3);
	
	int a;
	
	a =v1;
	v1=v2;
	v2=v3;
	v3= a;
	
	printf("\nValores intercambiados:\n1:%d \n2:%d \n3:%d",v1,v2,v3);
	
	return 0;
}
